{
  "manifest_version": 2,

  "name": "Redacted",
  "description": "This extension allows you to filter phrases that annoy you in government-style redaction (thick black lines).",
  "version": "0.2.0",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.min.js", "content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"

  },
  
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage"
  ]
}